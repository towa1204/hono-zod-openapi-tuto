{
  "imports": {
    "hono": "npm:hono",
    "zod": "npm:zod",
    "@hono/zod-openapi": "npm:@hono/zod-openapi",
    "@hono/swagger-ui": "npm:@hono/swagger-ui"
  },
  "tasks": {
    "start": "deno run -A --env --unstable-kv main.ts",
    "start-dev": "deno run -A --env=.env.dev --unstable-kv main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
